@{
    ViewData["Title"] = "Situação Do Cliente";
}

<div class="container col-md-8">
    <div class="row justify-content-center">
        <div class="col-md-4">
        </div>
        <div class="col-md-4">
            <h3 id="titulo-verificacao">@ViewData["Title"]</h3>
        </div>
        <div class="col-md-4">
        </div>
        <hr />
    </div>
    <div class="row justify-content-center">
        <div class="col-md-4">
            <div class="form-group">
                <div>
                    <a asp-controller="Demandeds" asp-action="Create" class="btn btn-outline-success">Cliente Já Cadastrado</a>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <div>
                    <a asp-controller="Clients" asp-action="Create" class="btn btn-outline-info">Cadastrar Cliente</a>
                </div>
            </div>
        </div>
    </div>
</div>

